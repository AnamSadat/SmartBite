{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./", // Ganti rootDir menjadi root folder proyek
    "outDir": "./dist",
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*", // Sertakan file dalam folder src
    "prisma/**/*", // Sertakan file dalam folder prisma
    "src/express.d.ts"
, "seed.ts"  ],
  "exclude": ["node_modules"]
}
